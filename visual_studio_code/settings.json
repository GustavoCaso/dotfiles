{
    "breadcrumbs.enabled": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.sideBar.location": "left",
    "workbench.colorTheme": "Atom One Dark",
    "workbench.editor.enablePreview": false,
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "task.autoDetect": "off",
    "editor.tabSize": 2,
    "editor.tabCompletion": "on",
    "editor.snippetSuggestions": "top",
    "editor.fontLigatures": true,
    "editor.fontFamily": "Monoid",
    "editor.minimap.enabled": false,
    "editor.fontWeight": "400",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        ".bundle/**": true,
        ".byebug_history/**": true,
        ".dev/**": true,
        "node_modules/**": true,
        "**/.git": false,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "search.exclude": {
        ".byebug_history/**": true,
        ".shadowenv.d/**": true,
        "**/.git": true,
        "**/vendor/*": true,
        "node_modules/**": true,
        "tmp/**": true
    },
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "ruby.intellisense": "rubyLocate",
    "ruby.codeCompletion": "rcodetools",
    "ruby.useBundler": true,
    "ruby.useLanguageServer": true,
    "ruby.lint": {
        "rubocop": {
            "useBundler": true
        },
    },
    "ruby.locate": {
        "include": "**/*.rb",
        "exclude": "{**/@(test|spec|tmp|vendor|.*),**/@(test|spec|tmp|vendor|.*)/**,**/*_spec.rb}"
    },
    "go.buildOnSave": "off",
    "go.formatTool": "goimports",
    "go.lintOnSave": "file",
    "go.useLanguageServer": true,
    "C_Cpp.updateChannel": "Insiders",
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.fileHistory.location": "scm",
    "gitlens.views.lineHistory.location": "scm",
    "gitlens.views.compare.location": "scm",
    "gitlens.views.search.location": "scm",
    "gitlens.blame.avatars": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.format": "${message}",
    "gitlens.hovers.avatars": false,
    "gitlens.blame.heatmap.location": "left",
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.statusBar.command": "gitlens.toggleCodeLens",
    "go.toolsManagement.autoUpdate": true,
    "gopls.env": {
        "GOFLAGS": "-tags=integration"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.project.importOnFirstTimeStartup": "automatic",
    "go.enableCodeLens": {
        "references": false,
        "runtest": true
    },
}
